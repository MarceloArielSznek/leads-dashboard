                roundDiscountPercent(percent) {
                    const integer = Math.floor(percent);
                    const decimal = percent - integer;
                    
                    if (decimal >= 0.8) {
                        // Round upward when >= 0.8
                        return integer + 1;
                    } else if (decimal >= 0.3 && decimal <= 0.7) {
                        // Round to .5 when between 0.3-0.7
                        return integer + 0.5;
                    } else {
                        // Round downward when 0.0-0.3
                        return integer;
                    }
